@model Service

<div class="container">
	<form method="post" enctype="multipart/form-data">
		<div class="form-group">
			<label asp-for="AboutTitle">Title:</label>
			<input type="text" class="form-control" asp-for="AboutTitle" />
			<span asp-validation-for="AboutTitle" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="AboutText">Text:</label>
			<input type="text" class="form-control" asp-for="AboutText" />
			<span asp-validation-for="AboutText" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="VideoLink">Link:</label>
			<input class="form-control" asp-for="VideoLink" />
			<span asp-validation-for="VideoLink" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Number1">Number 1:</label>
			<input class="form-control" asp-for="Number1" />
			<span asp-validation-for="Number1" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Number2">Number 2:</label>
			<input class="form-control" asp-for="Number2" />
			<span asp-validation-for="Number2" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Email">Email:</label>
			<input class="form-control" asp-for="Email" />
			<span asp-validation-for="Email" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="Address">Address:</label>
			<input class="form-control" asp-for="Address" />
			<span asp-validation-for="Address" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="HeaderLogo">HeaderLogo:</label>
			<input class="form-control" asp-for="HeaderLogo" />
			<span asp-validation-for="HeaderLogo" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="FooterLogo">FooterLogo:</label>
			<input class="form-control" asp-for="FooterLogo" />
			<span asp-validation-for="FooterLogo" class="text text-danger"></span>
		</div>
		<div class="form-group">
			<label asp-for="@Model.file">File:</label>
			<input type="file" class="form-control" asp-for="@Model.file" />
			<span asp-validation-for="file" class="text text-danger"></span>
		</div>
		@Html.AntiForgeryToken()
		<button type="submit">Update</button>
	</form>
</div>
